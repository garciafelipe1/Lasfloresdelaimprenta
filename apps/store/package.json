{
  "name": "@floreria/store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "exports": {
    "./types": "./src/shared/types.ts",
    "./constants": "./src/shared/constants.ts"
  },
  "scripts": {
    "build": "pnpm medusa build && npm run resolve:aliases && ln -s .medusa/server/public/ public",
    "predeploy": "pnpm medusa db:migrate",
    "start": "medusa start",
    "dev": "medusa develop",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "dev:email": "email dev --dir ./src/modules/resend/emails --port 9001",
    "resolve:aliases": "tsc --showConfig -p tsconfig.json > tsconfig.resolved.json && tsc-alias -p tsconfig.resolved.json && rimraf tsconfig.resolved.json"
  },
  "dependencies": {
    "@formatjs/intl": "3.1.6",
    "@medusajs/admin-sdk": "2.7.0",
    "@medusajs/cli": "2.7.0",
    "@medusajs/dashboard": "2.7.0",
    "@medusajs/framework": "2.7.0",
    "@medusajs/icons": "2.7.0",
    "@medusajs/js-sdk": "2.7.0",
    "@medusajs/medusa": "2.7.0",
    "@medusajs/ui": "4.0.13",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@nicogorga/medusa-payment-mercadopago": "0.2.5",
    "@react-email/components": "0.3.1",
    "awilix": "^8.0.1",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "mercadopago": "2.7.0",
    "pg": "^8.13.0",
    "react-quill": "2.0.0",
    "recharts": "^2.15.3",
    "resend": "4.7.0",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@hookform/resolvers": "5.2.2",
    "@medusajs/test-utils": "2.7.0",
    "@mikro-orm/cli": "6.4.3",
    "@react-email/preview-server": "4.2.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@tanstack/react-query": "5.64.2",
    "@types/cors": "2.8.19",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "clsx": "^2.1.1",
    "dotenv": "16.5.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^4.2.3",
    "react-hook-form": "7.65.0",
    "react-router-dom": "6.20.1",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53",
    "zod": "3.22.4"
  },
  "engines": {
    "node": ">=20"
  }
}
